{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:react/jsx-runtime",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
      ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2020
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": [2, "always"]
    },
    "settings": {
        "react": {
          "version": "detect"
        }
    }
}